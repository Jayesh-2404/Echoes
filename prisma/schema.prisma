generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Message{
  id           String @id @default(cuid())
  recipientId  String
  message      String
  ipAddress    String
  createdAt    DateTime  @default(now())


  @@index([recipientId])
}
